{
  "expo": {
    "name": "Luz do Saber Fundamental",
    "slug": "lds-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
